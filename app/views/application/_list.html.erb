<%= link_to category.name, county_category_path(@county, category), :class => "list-group-item list-group-item-action bg-light text-truncate" %>

<% category.subcategories.each do |subcategory| %>
  <%= link_to subcategory.name, county_subcategory_path(@county, subcategory), :class => "list-group-item list-group-item-action text-truncate" %>
<% end %>
